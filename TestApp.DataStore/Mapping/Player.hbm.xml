<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    namespace="TestApp.DataStore.Entities" assembly="TestApp.DataStore">

  <class name="PlayerEntity" table="Player">

    <id name="Id">
      <column name="Id" sql-type="Guid" not-null="true"/>
      <generator class="guid"/>
    </id>

    <property name="FullName">
      <column name="FullName" length="32" not-null="true" />
    </property>

    <property name="Position">
      <column name="Position" sql-type="Byte" />
    </property>
    <property name="Age" />
    <property name="ClubId"  sql-type="Guid"/>
  </class>
</hibernate-mapping>